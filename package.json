{
	"name": "@jtmdias/frontend",
	"private": true,
	"workspaces": [
		"packages/*",
		"docs"
	],
	"engines": {
		"node": ">= 20",
		"npm": ">= 10"
	},
	"scripts": {
		"all:build": "lerna run build",
		"all:test": "lerna run test",
		"all:upgrade": "npx npm-check-updates",
		"docs:start": "cd ./docs && yarn start",
		"docs:build": "cd ./docs && yarn build",
		"docs:serve": "cd ./docs && yarn serve",
		"reinstall": "rimraf node_modules && yarn install",
		"project-graph": "npx nx graph",
		"create-changeset": "changeset",
		"version": "changeset version",
		"prerelease": "yarn all:build --if-present",
		"release": "changeset publish"
	},
	"devDependencies": {
		"@changesets/changelog-github": "^0.5.0",
		"@changesets/cli": "^2.27.1",
		"@types/node": "^20.11.20",
		"app-root-path": "^3.1.0",
		"cypress": "^13.6.4",
		"lerna": "^8.1.2",
		"react-router-dom": "^6.22.3",
		"rimraf": "^5.0.5",
		"ts-node": "^10.9.2",
		"typescript": "^5.3.3",
		"vite-tsconfig-paths": "^4.3.1"
	}
}
